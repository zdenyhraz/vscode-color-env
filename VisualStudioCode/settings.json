{
    "cmake.generator": "Ninja",
    "cmake.parallelJobs": 6,
    "cmake.configureOnOpen": false,
    "C_Cpp.enhancedColorization": "Enabled",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "editor.fontFamily": "Cascadia Code",
    "editor.fontSize": 14,
    "editor.fontWeight": "600",
    "terminal.integrated.fontFamily": "Cascadia Code PL",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": 600,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "editor.minimap.enabled": false,
    "editor.minimap.scale": 2,
    "terminal.integrated.cursorBlinking": true,
    "editor.cursorBlinking": "expand",
    "window.zoomLevel": 1,
    "output.smartScroll.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "type": {
                "foreground": "#4effacc7"
            },
            "class": {
                "foreground": "#4effacc7"
            },
            "namespace": {
                "foreground": "#4effacc7"
            },
            "templateType": {
                "foreground": "#4effacc7"
            },
            "variable": {
                "foreground": "#D4D4D4"
            },
            "enumMember": {
                "foreground": "#fe4450"
            },
            "function": {
                "foreground": "#ff8119"
            },
            "method": {
                "foreground": "#ffae00"
            },
            "magicFunction": {
                "foreground": "#ffae00"
            },
            "memberOperatorOverload": {
                "foreground": "#ffae00"
            },
            "operatorOverload": {
                "foreground": "#ff8119"
            },
            "parameter": {
                "foreground": "#ffffff60"
            },
            "property": {
                "foreground": "#c548ff"
            },
            "macro": {
                "foreground": "#C586C0",
                "fontStyle": "italic"
            },
        }
    },
    "workbench.colorCustomizations": {
        "terminal.ansiBlue": "#0077ff",
        "terminal.ansiGreen": "#5eb140",
        "terminal.ansiYellow": "#fac13b",
        "sideBar.foreground": "#d1d1d1",
        "sideBar.background": "#0f0f0f",
        "editor.background": "#0f0f0f",
        "editor.selectionHighlightBackground": "#3b3b3b",
        "editor.selectionHighlightBorder": "#868686",
        "editor.lineHighlightBackground": "#0f0f0f",
        "editor.lineHighlightBorder": "#282828",
        "editorGutter.background": "#0f0f0f",
        "editorLineNumber.foreground": "#868686",
        "editorLineNumber.activeForeground": "#d1d1d1",
        "panel.background": "#0f0f0f",
        "panel.border": "#3b3b3b",
        "banner.background": "#0f0f0f",
        "statusBar.foreground": "#ffffff",
        "statusBar.background": "#282828",
        "statusBar.debuggingBackground": "#df8634",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.noFolderBackground": "#4bb47b",
        "statusBar.noFolderForeground": "#ffffff",
        "statusBarItem.hoverBackground": "#3b3b3b",
        "statusBarItem.remoteBackground": "#7289da",
        "statusBarItem.remoteForeground": "#ffffff",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#868686",
        "activityBar.background": "#0f0f0f",
        "activityBarBadge.foreground": "#ffffff",
        "activityBarBadge.background": "#7289da",
        "titleBar.activeBackground": "#0f0f0f",
        "titleBar.activeForeground": "#d1d1d1",
        "titleBar.inactiveBackground": "#0f0f0f",
        "titleBar.inactiveForeground": "#d1d1d1",
        "menu.foreground": "#d1d1d1",
        "menu.background": "#282828",
        "breadcrumb.background": "#282828",
        "breadcrumb.foreground": "#868686",
        "breadcrumb.focusForeground": "#d1d1d1",
        "breadcrumb.activeSelectionForeground": "#d1d1d1",
        "editorGroupHeader.tabsBackground": "#0f0f0f",
        "tab.activeForeground": "#ffffff",
        "tab.inactiveForeground": "#868686",
        "tab.activeBackground": "#282828",
        "tab.hoverBackground": "#282828",
        "tab.inactiveBackground": "#0f0f0f",
        "tab.border": "#00000000",
        "tab.activeBorder": "#00000000",
        "tab.activeBorderTop": "#00000000",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.type",
                    "entity.name.namespace",
                    "entity.name.namespace.cpp",
                    "entity.name.type.class.templated",
                    "entity.name.scope-resolution.cpp",
                    "entity.name.scope-resolution.function.call.cpp",
                    "entity.name.scope-resolution.parameter.cpp",
                    "entity.name.scope-resolution.function.definition.cpp",
                    "punctuation.separator.scope-resolution.cpp",
                    "punctuation.separator.scope-resolution.function.call.cpp",
                    "punctuation.separator.scope-resolution.template.call.cpp",
                    "support.type.property-name",
                    "support.type.property-name.json",
                    "support.type.property-name.json punctuation",
                    "keyword.control.directive",
                    "markup.heading",
                    "punctuation.section.block.begin.bracket.curly.class.cpp",
                    "punctuation.section.block.end.bracket.curly.class.cpp",
                    "punctuation.section.block.begin.bracket.curly.struct.cpp",
                    "punctuation.section.block.end.bracket.curly.struct.cpp",
                    "punctuation.section.block.begin.bracket.curly.enum.cpp",
                    "punctuation.section.block.end.bracket.curly.enum.cpp",
                    "storage.source.cmake",
                    "meta.function.definition.cpp"
                ],
                "settings": {
                    "foreground": "#4effacc7"
                }
            },
            {
                "scope": [
                    "entity.name.function.member",
                    "entity.name.function.operator.member",
                    "punctuation.separator.dot-access",
                    "punctuation.separator.pointer-access",
                    "entity.other.attribute-name.pragma",
                    "punctuation.section.arguments.begin.bracket.round.function.member",
                    "punctuation.section.arguments.end.bracket.round.function.member",
                    "support.function",
                    "punctuation.section.block.begin.bracket.curly.function.definition.cpp",
                    "punctuation.section.block.end.bracket.curly.function.definition.cpp",
                    "punctuation.section.parameters.begin.bracket.round.cpp",
                    "punctuation.section.parameters.end.bracket.round.cpp",
                    "punctuation.section.parameters.begin.bracket.round.special.constructor.cpp",
                    "punctuation.section.parameters.end.bracket.round.special.constructor.cpp",
                    "punctuation.section.parameters.begin.bracket.round.special.member.destructor.cpp",
                    "punctuation.section.parameters.end.bracket.round.special.member.destructor.cpp",
                    "punctuation.section.block.begin.bracket.curly.function.definition.special.constructor.cpp",
                    "punctuation.section.block.end.bracket.curly.function.definition.special.constructor.cpp",
                    "punctuation.section.block.begin.bracket.curly.function.definition.special.member.destructor.cpp",
                    "punctuation.section.block.end.bracket.curly.function.definition.special.member.destructor.cpp",
                    "string.quoted.double.json.comments",
                    "punctuation.definition.string.begin.json.comments",
                    "punctuation.definition.string.end.json.comments",
                    "constant.numeric.json.comments",
                    "constant.language.json.comments",
                    "source.cmake",
                    "variable.source.cmake",
                    "entity.name.function.lua"
                ],
                "settings": {
                    "foreground": "#ffae00"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "entity.name.function.member.static",
                    "entity.name.function.operator",
                    "variable.other.property.static",
                    "variable.other.global",
                    "punctuation.section.arguments.begin.bracket.round.function.call.cpp",
                    "punctuation.section.arguments.end.bracket.round.function.call.cpp",
                ],
                "settings": {
                    "foreground": "#ff8119"
                }
            },
            {
                "scope": [
                    "entity.name.function.preprocessor",
                    "entity.other.attribute-name.pragma.preprocessor.cpp",
                    "punctuation.definition.parameters.begin.preprocessor.cpp",
                    "punctuation.definition.parameters.end.preprocessor.cpp",
                ],
                "settings": {
                    "foreground": "#C586C0",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "variable.parameter",
                ],
                "settings": {
                    "foreground": "#ffffff60",
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "variable.other.enummember",
                    "keyword.operator.logical",
                    "keyword.operator.ternary",
                    "keyword.control.return",
                    "keyword.control.break",
                    "keyword.control.continue",
                    "keyword.control.throw",
                    "keyword.control.try",
                    "keyword.control.catch",
                    "keyword.operator.new",
                    "keyword.operator.delete",
                    "keyword.operator.delete.array",
                    "keyword.operator.or",
                    "keyword.operator.and",
                    "keyword.operator.not",
                    "keyword.operator.wordlike",
                    "constant.numeric",
                    "keyword.other.unit",
                    "entity.name.function.operator.cpp",
                ],
                "settings": {
                    "foreground": "#fe4450"
                }
            },
            {
                "scope": [
                    "comment",
                ],
                "settings": {
                    "foreground": "#6B7A84",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": [
                    "storage.type.class.doxygen",
                ],
                "settings": {
                    "foreground": "#00ec0c"
                }
            },
            {
                "scope": [
                    "variable.other.local",
                    "variable.other.readwrite",
                    "meta.paragraph.markdown",
                ],
                "settings": {
                    "foreground": "#D4D4D4"
                }
            },
            {
                "scope": [
                    "variable.other.property",
                    "punctuation.section.arguments.begin.bracket.round.function.call.initializer.cpp",
                    "punctuation.section.arguments.end.bracket.round.function.call.initializer.cpp",
                ],
                "settings": {
                    "foreground": "#c548ff"
                }
            },
            {
                "scope": [
                    "string",
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end"
                ],
                "settings": {
                    "foreground": "#CE9178"
                }
            },
            {
                "scope": [
                    "constant.character.escape",
                ],
                "settings": {
                    "foreground": "#D7BA7D"
                }
            },
            {
                "scope": [
                    "keyword.control",
                    "keyword.operator.assignment",
                    "keyword.operator.assignment.cpp",
                    "keyword.operator.increment",
                    "keyword.operator.bitwise",
                    "keyword.operator.arithmetic",
                    "keyword.operator.arithmetic.cpp",
                    "keyword.operator",
                    "keyword.control.directive.include",
                    "keyword.control.directive.define.cpp",
                    "keyword.control.directive.pragma.cpp",
                    "keyword.other",
                    "constant.language",
                    "storage.type",
                    "storage.modifier",
                    "storage.modifier.specifier",
                    "storage.modifier.pointer",
                    "storage.modifier.reference",
                    "storage.modifier.virtual",
                    "storage.modifier.static",
                    "punctuation.section.angle-brackets.start.template.definition",
                    "punctuation.section.angle-brackets.end.template.definition",
                    "punctuation.section.angle-brackets.begin.template.call",
                    "punctuation.section.angle-brackets.end.template.call",
                    "punctuation.section.parens.begin.bracket.round",
                    "punctuation.section.parens.end.bracket.round",
                    "punctuation.definition.capture.begin.lambda.cpp",
                    "punctuation.definition.capture.end.lambda.cpp",
                    "punctuation.definition.parameters.begin.lambda.cpp",
                    "punctuation.definition.parameters.end.lambda.cpp",
                    "punctuation.section.block.begin.bracket.curly.lambda",
                    "punctuation.section.block.end.bracket.curly.lambda",
                    "punctuation.definition.begin.bracket.square",
                    "punctuation.definition.end.bracket.square",
                    "punctuation.separator.colon.range-based.cpp",
                    "punctuation.separator.colon.inheritance",
                    "markup.underline",
                    "variable.language",
                    "meta.template.call.cpp",
                    "keyword.operator.comparison",
                    "keyword.operator.comparison.cpp",
                    "keyword.operator.bitwise.shift",
                    "keyword.operator.bitwise.shift.cpp",
                    "keyword.operator.increment.cpp",
                    "keyword.control.flow.python",
                    "keyword.control.import.python",
                    "keyword.control.from.python",
                    "keyword.cmake",
                    "entity.other.attribute",
                    "punctuation.section.attribute"
                ],
                "settings": {
                    "foreground": "#569CD6",
                    "fontStyle": ""
                }
            }
        ]
    },
    "workbench.colorTheme": "Discord Theme",
    "clang-format.assumeFilename": ".clang-format",
    "clang-format.language.apex.enable": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "git.confirmSync": false,
    "tabnine.experimentalAutoImports": true,
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c11",
    "qttools.loglevel": "error",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "c-cpp-flylint.cppcheck.suppressions": [
        "syntaxError",
        "misc-implicitlyVirtual"
    ],
    "c-cpp-flylint.cppcheck.inconclusive": true,
    "c-cpp-flylint.cppcheck.addons": [
        "cert",
        "misc"
    ],
    "c-cpp-flylint.cppcheck.extraArgs": [
        "--enable=warning,style,performance,portability,information"
    ],
    "c-cpp-flylint.excludeFromWorkspacePaths": [
        "libs"
    ],
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.lizard.enable": false,
    "c-cpp-flylint.clang.enable": false,
    "gitlens.hovers.currentLine.over": "line",
    "cmake.configureSettings": {
        //"CMAKE_TOOLCHAIN_FILE": "${workspaceFolder}/toolchain-ppc-p2020.cmake",
    },
    "cmake.environment": {
        //"FIRMWARE_SHAREPOINT": "${workspaceFolder}/results",
    },
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--indent-size",
        "2",
        "--max-line-length",
        "120",
        "--experimental"
    ],
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
}
